{
  "name": "countr-access-manager",
  "main": "build",
  "license": "UNLICENSE",
  "scripts": {
    "build": "tsc",
    "docker": "npm run docker:build && npm run docker:up",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:start": "npm run docker:up",
    "docker:stop": "npm run docker:down",
    "docker:up": "docker-compose up -d",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "start": "node .",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@typegoose/typegoose": "12.18.0",
    "bufferutil": "4.0.9",
    "discord.js": "14.22.1",
    "dotenv": "17.2.3",
    "mongoose": "8.17.2",
    "winston": "3.18.2",
    "winston-daily-rotate-file": "5.0.0",
    "zlib-sync": "0.1.10"
  },
  "devDependencies": {
    "@tsconfig/node22": "22.0.2",
    "@tsconfig/strictest": "2.0.6",
    "@types/node": "22.18.8",
    "eslint": "9.36.0",
    "eslint-config-promise": "github:promise/eslint-config",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.17.1"
}
