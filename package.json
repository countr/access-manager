{
  "name": "countr-access-manager",
  "main": "build",
  "license": "UNLICENSE",
  "scripts": {
    "build": "tsc",
    "docker": "npm run docker:build && npm run docker:up",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:start": "npm run docker:up",
    "docker:stop": "npm run docker:down",
    "docker:up": "docker-compose up -d",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "start": "node .",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@typegoose/typegoose": "11.5.0",
    "bufferutil": "4.0.7",
    "discord.js": "14.13.0",
    "dotenv": "16.3.1",
    "erlpack": "github:discord/erlpack",
    "mongoose": "7.5.2",
    "utf-8-validate": "6.0.3",
    "winston": "3.10.0",
    "winston-daily-rotate-file": "4.7.1",
    "zlib-sync": "0.1.8"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "18.17.16",
    "eslint": "8.49.0",
    "eslint-config-promise": "github:promise/eslint-config",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "extends": "promise"
  }
}
